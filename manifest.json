{
  "name": "arrowHeadCursorExtension",
  "version": "1.0.0",
  "manifest_version": 3,
  "web_accessible_resources": [{
    "resources": [
      "img/cursor1.png",
      "img/cursor2.png",
      "img/cursor3.png",
      "img/cursor4.png",
      "img/cursor5.png",
      "img/cursor6.png",
      "img/cursor7.png"
    ],
    "matches": [
      "https://*/*",
      "http://*/*"
    ]
  }
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "description": "カーソルを新石器時代の石鏃にするChrome拡張",
  "content_scripts": [{
    "matches": [
      "https://*/*",
      "http://*/*"
    ],
    "js": [
      "content.js"
    ]
  }]
}
